<?xml version="1.0" encoding="utf-8"?>
<configure 
    xmlns="http://namespaces.zope.org/zope"
    xmlns:browser="http://namespaces.zope.org/browser"
    xmlns:zcml="http://namespaces.zope.org/zcml"
    >

    <adapter
        factory=".info.WinServiceInfo"
        for=".WinService.WinService"
        provides=".interfaces.IWinServiceInfo"
        />

    <adapter
        factory=".info.WinProcInfo"
        for=".WinProc.WinProc"
        provides=".interfaces.IWinProcInfo"
        />

    <adapter
        provides=".monitor.ITypeperfSc1Info"
        for=".datasources.TypeperfSc1DataSource.TypeperfSc1DataSource"
        factory=".monitor.SingleCounterInfo"
        />

    <adapter
        provides=".monitor.IPowershellGetCounterInfo"
        for=".datasources.PowershellGetCounterDataSource.PowershellGetCounterDataSource"
        factory=".monitor.SingleCounterInfo"
        />

    <!-- Browser -->
    <include package=".browser"/>

</configure>
